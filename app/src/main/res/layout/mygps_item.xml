<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/gps_iv_image"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:background="@drawable/round_shape_background"
        android:scaleType="fitXY"
        android:src="@drawable/category_icon0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.222"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/gps_Tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="알바"
        android:textColor="@color/DimGray"
        android:textSize="12dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="22.5dp"
        app:layout_constraintTop_toBottomOf="@id/gps_iv_image"
        app:layout_constraintEnd_toEndOf="@id/gps_iv_image"
        app:layout_constraintStart_toStartOf="@id/gps_iv_image"
        tools:layout_editor_absoluteY="60dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>